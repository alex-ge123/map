<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wafersystems.virsical.map.mapper.SvgStateMapper">

  <!-- 通用查询映射结果 -->
  <resultMap id="BaseResultMap" type="com.wafersystems.virsical.map.entity.SvgState">
    <id column="svg_state_id" property="svgStateId"/>
    <result column="tenant_id" property="tenantId"/>
    <result column="svg_id" property="svgId"/>
    <result column="svg_state_name" property="svgStateName"/>
    <result column="svg_state_code" property="svgStateCode"/>
    <result column="svg_state_element" property="svgStateElement"/>
    <result column="view_box" property="viewBox"/>
    <result column="create_time" property="createTime"/>
    <result column="update_time" property="updateTime"/>
  </resultMap>

  <!-- 通用查询结果列 -->
  <sql id="Base_Column_List">
        svg_state_id, tenant_id, svg_id, svg_state_name, svg_state_code, svg_state_element, create_time, update_time
    </sql>

  <select id="getBySvgIdAndStateCode" resultType="com.wafersystems.virsical.map.entity.SvgState">
    select * from svg_state where svg_id = #{svgId} and svg_state_code = #{svgStateCode}
  </select>
</mapper>
